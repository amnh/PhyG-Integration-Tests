--softwired, fuse(), inputting prefastc file
set(seed:1634561640)
set(graphtype:softwired)
set(graphfactor:nopenalty)
read(prefastc:"chel-wide.fastc")
read(prefastc:"chel-huge.fastc")
build(distance, rdwag, block, graph, replicates:10, displaytrees:10)
refine(netadd, maxnetedges:3)
fuse(all)
report("t492.csv", data, overwrite)
report("t492.tre", graphs, newick, graphs, overwrite)
report("t492_gv.tre", dotpdf, graphs, overwrite)
report("t492_dt.tre", displaytrees, graphs, overwrite)