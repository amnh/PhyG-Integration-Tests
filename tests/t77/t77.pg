--build(distance, rdwag, block), inputting amino acid file
set(seed:1634561640)
read(aminoacid:"../data/metazoa-aa-1.fas")
read(aminoacid:"../data/metazoa-aa-2.fas")
build(distance, rdWag, block, cun, displaytrees:5, replicates:100) 
report("t77.csv", data, overwrite)
report("t77.tre", newick, graphs, overwrite)
report("t77_rec-Adams.tre", newick, reconcile, method:Adams, overwrite)
report("t77_rec-cun.tre", newick, reconcile, method:cun, threshold:10, overwrite)
report("t77_rec-eun.tre", newick, reconcile, method:eun, threshold:100, overwrite)
report("t77_rec-maj.tre", newick, reconcile, method:majority, threshold:75, overwrite)
report("t77_rec-cs.gv", dot, dotpdf, reconcile, method:strict, overwrite)
report("t77_rec-cs.tre", newick, reconcile, method:strict, overwrite)