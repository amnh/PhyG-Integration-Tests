--softwired, fuse(), inputting tcm file (dayhoff) with amino acids
set(seed:1634561640)
set(graphtype:softwired)
set(graphfactor:nopenalty)
read(prefasta:"../data/metazoa-aa-1_missing.fas")
read(prefasta:"../data/metazoa-aa-2_missing.fas")
read(newick:"../t89/t89.tre")
