--softwired, fuse(), inputting tcm and nucleotide files
set(seed:1634561640)
set(graphtype:softwired)
set(graphfactor:nopenalty)
read(nucleotide:"chel-prealigned.fas", tcm:"sg1t4.mat")
read(nucleotide:"chel_cox1aln.fasta", tcm:"sg2t1.mat")
build(distance, rdwag, block, graph, replicates:10, displaytrees:10)
refine(netadd, maxnetedges:3)
fuse(spr)
report("t494.csv", data, overwrite)
report("t494.tre", newick, graphs, overwrite)
report("t494_gv.tre", dotpdf, graphs, overwrite)
report("t494_dt.tre", displaytrees, graphs, overwrite)