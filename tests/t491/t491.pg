--softwired, fuse(), inputting prefasta file
set(seed:1634561640)
set(graphtype:softwired)
set(graphfactor:nopenalty)
read(prefasta:"chel-prealigned.fas")
read(prefasta:"chel_cox1aln.fasta")
read(prefasta:"metazoa-aa-1.fas")
read(prefasta:"metazoa-aa-2.fas")
build(distance, rdwag, block, graph, replicates:10, displaytrees:10)
refine(netadd, maxnetedges:3)
fuse(spr)
report("t491.csv", data, overwrite)
report("t491.tre", graphs, newick, overwrite)
report("t491_gv.tre", dotpdf, graphs, overwrite)
report("t491_dt.tre", displaytrees, graphs, overwrite)